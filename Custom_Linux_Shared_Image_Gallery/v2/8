#%Module1.0
proc ModulesHelp { } {
global dotversion

puts stderr "\tJava 8"
}

module-whatis "Java 8"
prepend-path PATH /usr/lib/jvm/java-1.8.0-openjdk-1.8.0.265.b01-0.el8_2.x86_64/bin
setenv JAVA_HOME /usr/lib/jvm/java-1.8.0-openjdk-1.8.0.265.b01-0.el8_2.x86_64/bin/java
setenv JRE_HOME /usr/lib/jvm/java-1.8.0-openjdk-1.8.0.265.b01-0.el8_2.x86_64/bin/java
